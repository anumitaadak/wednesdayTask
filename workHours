function workHours(){
number=$((RANDOM%3))
case $number in
   0)
   echo "Employee is absent"
   workingHour=0
   ;;
   1)
   echo "Employee is doing part-time"
   workingHour=4
   ;;
   *)
   echo "employee is doing full-time"
   workingHour=8
   ;;
esac
return 0
return 1
return *
}

workHours $1 

   
